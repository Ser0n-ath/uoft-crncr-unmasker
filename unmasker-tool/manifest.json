{
    "manifest_version": 3,
    "name": "CR/NCR Unmasker",
    "version": "1.0",
    "description": "A simple tool to view your CR/NCR marks on degree explorer",
    "action": {
      "default_popup": "popup.html",
      "default_title": "CR/NCR Courses"
    },
    "host_permissions": [
      "https://degreeexplorer.utoronto.ca/*"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }